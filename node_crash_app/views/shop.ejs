<%- include('includes/header.ejs') %>
        <main>
            <p>Product List</p>
            <% if (products.length> 0) { %> <% products.forEach(item=> { %>
            <p><%= item.title %></p>
            <form action="/" method="post">
                <input type="hidden" value="<%= item._id %>" name="productId"/>
                <button type="submit">Delete</button>
            </form>
            <% }) %> <% } else{ %>
            <p>Product Not found</p>
            <% } %>
        </main>
    </body>
</html>
